<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
        <f:entry title="${%Jacoco coverage counter type}" field="jacocoCoverageCounter">
          <select name="jacocoCoverageCounter">
            <f:option value="INSTRUCTION">${%Instruction}</f:option>
            <f:option value="LINE">${%Line}</f:option>
            <f:option value="COMPLEXITY">${%Complexity}</f:option>
            <f:option value="METHOD">${%Method}</f:option>
            <f:option value="CLASS">${%Class}</f:option>
          </select>
        </f:entry>
        <f:entry title="${%Publish result as}" field="publishResultAs">
          <select name="publishResultAs">
            <f:option value="comment">${%Comment}</f:option>
            <f:option value="statusCheck">${%Status Check}</f:option>
          </select>
        </f:entry>
        <f:entry title="${%Mode of comparing against master}" field="comparisonMode">
          <select name="comparisonMode">
            <f:option value="Strict">${%Strict}</f:option>
            <f:option value="threshold">${%Threshold}</f:option>
            <f:option value="tolerance">${%Tolerance}</f:option>
          </select>
        </f:entry>
</j:jelly>
